{
  "name": "copy-with",
  "version": "0.0.0",
  "main": "lib/index",
  "scripts": {
    "test": "mocha -r specs/testHelper.js specs/**/*.spec.js",
    "test:coverage": "BABEL_ENV=testcoverage nyc npm test",
    "build": "npm run clear:lib && npm run build:src",
    "build:src": "BABEL_DISABLE_CACHE=1 babel src -d lib --copy-files",
    "clear:lib": "rm -rf lib"
  },
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "chai": "^3.5.0",
    "dirty-chai": "^1.2.2",
    "mocha": "^3.0.2"
  },
  "dependencies": {
    "object-assign": "^4.1.0"
  }
}
